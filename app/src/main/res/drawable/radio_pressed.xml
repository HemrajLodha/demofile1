<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="10dp"
        android:height="2dp"
        android:left="6dp"
        android:top="11dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#12fff0" />
            </shape>
        </rotate>
    </item>

    <item
        android:width="19dp"
        android:height="2dp"
        android:left="11dp"
        android:top="7dp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="#12fff0" />
            </shape>
        </rotate>
    </item>
</layer-list>